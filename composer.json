{
    "name": "lebenlabs/simplestorage",
    "description": "Package para storage simplificado",
    "authors": [
        {
            "name": "Lebenlabs",
            "email": "devops@lebenlabs.com",
            "homepage": "https://github.com/leamasuero/simplestorage"
        }
    ],
    "homepage": "https://github.com/leamasuero/simplestorage",
    "keywords": ["Laravel", "Lebenlabs", "SimpleStorage"],
    "require": {
        "illuminate/support": "~5",
        "gedmo/doctrine-extensions": "^2.4",
        "laravel-doctrine/extensions": "1.0.*",
        "laravel-doctrine/migrations": "1.1.*",
        "laravel-doctrine/orm": "1.4.*",
        "laracasts/flash": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lebenlabs\\SimpleStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lebenlabs\\SimpleStorage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lebenlabs\\SimpleStorage\\Providers\\SimpleStorageServiceProvider"
            ],
            "aliases": {
                "SimpleStorage": "Lebenlabs\\SimpleStorage\\Facades\\SimpleStorage"
            }
        }
    }
}
